<% include!("../header.stpl"); %>

<section page-id="62" data-id="<%= category.id %>" data-title="Категория услуг <%= category.name %>" data-description="вебсервисы.рф: Категория услуг <%= category.name %>" data-uri="/services/<%= category.slug %>/" data-image="<% if category.image.is_some() { %><%= category.image.as_deref().unwrap() %><% } else { %>/static/images/img.jpg<% } %>" class="doc_title">
    <div class="relative container align-left">
        <div class="hs-line-4 font-alt">
            <a class="ajax" href="/service_categories/">Услуги</a>
            &nbsp;/&nbsp;
            <a><%= category.name %></a>
            <% include!("../generic/category_stat.stpl"); %>
        </div>
    </div>
</section>

<section class="">
<% if object_list.len() > 0 { %>
  <div class="is_paginate">
  <% for object in object_list.iter() { %>
    <% include!("../generic/default_service.stpl"); %>
  <% } %>
  <% if next_page_number > 0 { %>
    <div class="load centered m-1 next_page_list" data-link="/services/<%= category.slug %>/?page=<%= next_page_number %>">
      <img src="/static/images/preloader.gif">
    </div>
  <% } %>
  </div>
<% } else { %>
<div style="margin-top: 40px;">
    <div class="align-center">
        <span class="border" style="padding: 10px 15px;">Список пока пустой.</span>
    </div>
</div>
<% } %>
</section>
<div class="sidebar hidden">
    <% include!("sidebar.stpl"); %>
</div>
<% include!("../footer.stpl"); %>
