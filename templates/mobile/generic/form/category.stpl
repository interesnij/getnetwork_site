<div data-sum="<%= category.default_price %>" class="tab_<%= i + 1 %> fade tab-pane <% if i == 0 { %>in active<% } %>">
    <table class="table">
        <h5>
          <span data-pk="<%= category.id %>" data-type="serve_category" owner-pk="<%= object.id %>" owner-type="service" class="pointer get_object_info " tooltip="Справка" flow="up">
            <%= t_category.name %> на <%= category.name %>
          </span>
        </h5>
        <tbody>

        <% for serve in category.get_serves().iter() { %>
          <% if serve.is_default { %>
            <%
              if i == 0 {
                serves_ids.push_str(&serve.id.to_string());
                serves_ids.push_str(",");
              }
            %>
            <tr class="is_default hover">
            <td style="width:52%">
                <a class="get_object_info" data-pk="<%= serve.id %>" data-type="serve" owner-pk="<%= object.id %>" owner-type="service">
                    <%= serve.cat_name %> => <%= serve.name %> (<%= serve.id %>)
                </a>
            </td>
            <td class="price_td" style="width:12%"><span class="price"><%= serve.price %></span> ₽</td>
            <td style="width:12%">Часы: <%= serve.man_hours %></td>
            <td class="action_text" style="width:12%">Выбрано</td>
        </tr>
        <% } else { %>
          <tr class="select_serve pointer">
          <td style="width:55%">
              <a class="pointer underline get_object_info" data-pk="<%= serve.id %>" data-type="serve" owner-pk="<%= object.id %>" owner-type="service">
                  <%= serve.cat_name %> => <%= serve.name %> (<%= serve.id %>)
              </a>
          </td>
          <td class="price_td" style="width:15%"><span class="price"><%= serve.price %></span> ₽</td>
          <td style="width:15%">Часы: <%= serve.man_hours %></td>
          <td class="action_text" style="width:15%">Выбрать<% } %>
          </td>
      </tr>
        <% } %>
        </tbody>
    </table>
</div>
